    "7f793e2c-1629-466b-ac72-f8135446a51c": {
      "type": "rich-text",
      "blocks": {
        "7fdcbad9-dd7a-48e7-ad4a-6bf18adc29f2": {
          "type": "heading",
          "disabled": true,
          "settings": {
            "heading": "Talk about your brand",
