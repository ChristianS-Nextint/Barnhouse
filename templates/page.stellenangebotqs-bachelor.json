    "71fb1a2f-f9cb-45e4-a85b-40294a4ae1e6": {
      "type": "rich-text",
      "blocks": {
        "00a4b626-4f12-42d5-beef-42d2f736c277": {
          "type": "heading",
          "disabled": true,
          "settings": {
            "heading": "Mitarbeiter m\/w\/d für unsere Versandabteilung ca. 20 Std.\/Woche in flexibler Teilzeit",
            "heading_size": "h1",
            "heading_tag": "h2"
          }
        },
        "2c1cb6b7-763f-4c16-9c46-a3020421fc3b": {
          "type": "text",
          "settings": {
            "text": "<h3><strong>Damit überzeugst du uns:<\/strong><\/h3><p>Du befindest dich im Studium oder in einer Weiterbildung im Bereich Nachhaltigkeit.<\/p><p>Das Thema Nachhaltigkeit interessiert dich sehr.<\/p><p>Eine sehr gute Selbstorganisation sowie Durchsetzungsfähigkeit zeichnen dich aus.<\/p>",
            "text_size": "typeset",
            "secondary_color": false
          }
        }
      },
