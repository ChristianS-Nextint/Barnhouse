      "type": "rich-text",
      "blocks": {
        "21d6bb57-feb7-4251-8c2b-80df55f90ffd": {
          "type": "heading",
          "disabled": true,
          "settings": {
            "heading": "Mitarbeiter m\/w\/d für unsere Versandabteilung ca. 20 Std.\/Woche in flexibler Teilzeit",
            "heading_size": "h1",
            "heading_tag": "h2"
