    "b1717a83-3d26-4cbf-975f-a92f94ba7d61": {
      "type": "image-with-text",
      "blocks": {
        "template--18869337555280__b1717a83-3d26-4cbf-975f-a92f94ba7d61-168286389504683b83-0": {
          "type": "heading",
          "settings": {
            "heading": "Granola als Kindermüsli",
            "heading_size": "h2",
            "heading_tag": "h2"
          }
