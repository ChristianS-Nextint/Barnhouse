    "c79599f8-fccb-49da-9a9b-a76aaf66e8e0": {
      "type": "rich-text",
      "blocks": {
        "e0e7cc23-2c6b-40b1-9157-e0b80ad79934": {
          "type": "heading",
          "disabled": true,
          "settings": {
            "heading": "Talk about your brand",
            "heading_size": "h1",
